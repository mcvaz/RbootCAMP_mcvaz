geomGrowth = function(Ni=100,r=1.05,t=10,plot=TRUE){
N = c(Ni,rep(NA,t))
for(i in 1:t){N[i+1] = r*N[i]}
if(plot){plot(N,xlab="Time",type="l")}
return(N)
}
